@model List<string>
@{
    ViewData["Title"] = "Home Page";
}
<script src="https://cdn.plot.ly/plotly-latest.min.js"></script>

<h3>Юсерс </h3>

@foreach (var element in Model)
{
    <p>@Html.DisplayFor(m => element)</p>
}

<div id="myDiv"><!-- Plotly chart us be drawn inside this DIV --></div>
<script>
    var data = [
        {
            x: ["02-06-2017" , "04-06-2017"  , "05-06-2017", "07 - 06 - 2017", ],
            y: [@ViewBag.users, 500, 100,1000],   
            type: 'scatter'
        }
    ];

    Plotly.newPlot('myDiv', data, {}, { showSendToCloud: true });

</script>