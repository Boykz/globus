    @model IEnumerable<EntGlobus.Models.Satilim>
    @{
        ViewData["Title"] = "Satilim";
    }

    <a target="_blank" href="/dev/AddSatu">Qosu</a>
    <h2>Satilim</h2>

    <table class="table">
        <tr>
            <th>Aty</th>
            <th>Bagasy</th>
            <th>Tipi</th>
        </tr>

        @foreach (var md in Model)
        {
        <tr>

            <td>@md.Name</td>
            <td>@md.Price</td>
            <td>@md.Type   <a href="/dev/satdel?id=@md.Id"><span class="glyphicon glyphicon-trash"> </span></a>  <a><span class="glyphicon glyphicon-edit"></span></a></td>
        </tr>
                }
            
        </table>

