@model IEnumerable<EntGlobus.Models.SchoolDbFolder.SchoolClass>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/LiveLayout.cshtml";
}


<link rel="stylesheet" href="~/Live/css/font-awesome.css">
<link rel="stylesheet" href="~/Live/css/panSelect.css">

<div class="wrap">
    <div class="main">
        <header>
            <a href="~/LiveTest/LiveTest/Index" class="back"><i class="fa fa-chevron-left"></i></a> Онлайн сабақ
        </header>
        <div class="content">
            @foreach (var d in Model)
            {
                <a href="~/school/Index2/@d.Id" class="pan">
                    <div class="img-block">

                        <p>@d.ClassName</p>
                    </div>
                    <p class="panName"></p>
                </a>
            }
        </div>
    </div>
</div>

